{
  "name": "fixtab-backoffice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "serve": "serve -s build -l 4000",
    "storybook": "start-storybook -p 6006 -s public",
    "build": "react-scripts build",
    "build:css": "postcss src/styles/styles.scss -o postcss.css",
    "build:storybook": "build-storybook -s public",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}' --config ./.prettierrc",
    "test": "react-scripts test --watchAll=false",
    "test:w": "react-scripts test",
    "test:cv": "yarn test --coverage",
    "test:cv:msg": "yarn test:cv --verbose",
    "test:cv:w": "yarn test:w --coverage",
    "test:cv:msg:w": "yarn test:cv:w --verbose",
    "test:cv:open": "open coverage/lcov-report/index.html",
    "pre:commit": "yarn lint && yarn build && yarn build:css",
    "pre:push": "yarn test:cv",
    "husky:install": "husky install",
    "husky:add": "husky add"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "i18next": "^21.8.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-i18next": "^11.18.3",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "autoprefixer": "^10.4.8",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-mock": "^28.1.3",
    "jest-styled-components": "^7.1.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "sass": "^1.54.3",
    "serve": "^14.0.1",
    "storybook-dark-mode": "^1.1.0",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^28.0.7",
    "twin.macro": "^3.0.0-rc.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.tsx",
      "!src/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "index.tsx",
      "index.ts",
      "src/reportWebVitals.ts",
      "src/configs",
      "src/constants",
      "src/locales",
      "src/types",
      "Routes.tsx"
    ]
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "autoCssProp": false
    }
  }
}
